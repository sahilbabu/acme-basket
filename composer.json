{
  "name": "sahilbabu/acme-basket",
  "description": "ThriveCart Code Challenge (Acme Widget Co Basket)",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Mudassar Ali",
      "email": "sahil_bwp@yahoo.com"
    }
