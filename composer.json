    "php": "^8.2",
    "psr/log": "^1.1",
    "symfony/dependency-injection": "^7.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^11.2",
    "phpstan/phpstan": "^1.11",
    "symfony/var-dumper": "^7.1"
  }
}
